{
    "defaultSeverity": "error",
    "extends": "tslint-config-airbnb",
    "jsRules": {},
    "rules": {
        "eofline": false,
        "variable-name": {
            "options": [
                "allow-pascal-case"
            ]
        },
        "max-line-length": [
            true,
            {
                "limit": 120,
                "ignore-pattern": "^import [^,]+ from |^export | implements"
            }
        ]
    },
    "rulesDirectory": []
}